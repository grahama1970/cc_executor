{
  "hooks": {
    "pre_execute": [
      {
        "name": "Task List Pre-flight Check",
        "script": "src/cc_executor/hooks/task_list_preflight_check.py",
        "env": {
          "CLAUDE_FILE": "examples/03_with_hooks/task_list.md"
        }
      }
    ],
    "post_execute": [
      {
        "name": "Task List Completion Report",
        "script": "src/cc_executor/hooks/task_list_completion_report.py",
        "env": {
          "CLAUDE_TASK_LIST_FILE": "examples/03_with_hooks/task_list.md"
        }
      }
    ]
  }
}